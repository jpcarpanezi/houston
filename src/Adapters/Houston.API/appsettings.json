{
  "ConnectionStrings": {
    "Postgres": "postgres",
    "Redis": "redis",
    "RabbitMQ": "rabbit"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId", "EventTypeEnricher" ],
    "WriteTo": [{ "Name": "Console" }]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "TokenConfigurations": {
    "Audience": "houston",
    "Issuer": "houston",
    "Seconds": 1200,
    "FinalExpiration": 3600
  },
  "Cors": {
    "AllowedOrigins": [ "http://localhost:4200" ],
    "AllowedMethods": [ "GET", "POST", "PUT", "PATCH", "DELETE" ]
  },
  "AllowedHosts": "*"
}
